<Project InitialTargets="Discord_Net_ComponentDesigner">
    <Target Name="Discord_Net_ComponentDesigner">
        <PropertyGroup>
            <InterceptorsNamespaces>$(InterceptorsNamespaces);InlineComponent</InterceptorsNamespaces>
            <InterceptorsPreviewNamespaces>$(InterceptorsPreviewNamespaces);InlineComponent</InterceptorsPreviewNamespaces>
        </PropertyGroup>

        <ItemGroup>
            <CompilerVisibleProperty Include="EnableAutoRows" />
            <CompilerVisibleProperty Include="EnableAutoTextDisplay" />
        </ItemGroup>
    </Target>
</Project>